<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 我的 ASP.NET 应用程序</title>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    @Styles.Render("~/Content/css")
    <style type="text/css">
        ul li {
            float: left;
            text-decoration: none
        }
        a {
            text-decoration: none;
        }
    </style>
    
</head>

<body>
    <div style="float:none;width:100%;margin-top:-50px;">
        <div>
            <img src="~/image/alibaba.jpg" style="float:left" />

        </div>
        <div>
            <ul style="list-style:none;line-height:80px">
                <li><a href="@Url.Action("index","Home")"> 阿里云首页&nbsp;&nbsp; |&nbsp;&nbsp;</a></li>
                <li><a href="@Url.Action("about","Home")">阿里云首页 &nbsp;&nbsp;|&nbsp;&nbsp;</a></li>
                <li><a href="@Url.Action("contact","Home")">阿里云首页&nbsp;&nbsp; |&nbsp;&nbsp;</a></li>
                <li> @Html.ActionLink("阿里云首页","index", "Home") &nbsp;&nbsp;</li>
            </ul>
        </div>
    </div>
    <br />
    <br />
    <br />
    <div style="float:none"> <hr /> </div>

    <div class="container body-content" style="float:none">
        @RenderBody()
        <div style="width:100%;">
            <div style="float:left;margin-left:50px">
                <img src="~/image/alimail.jpg" />
            </div>
            <div style="float:right;margin-top:50px;margin-right:100px">
                @RenderSection("sessi",required:false)
            </div>
        </div>
    </div>


</body>
</html>
